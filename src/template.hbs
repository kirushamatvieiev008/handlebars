{{#each newFootball}}
  <li>
    <h2>{{name}}</h2>
    <p>{{age}}</p>
    <p>{{rating}}</p>
    <p>{{position}}</p>
    <ul>
      {{#each trophies}}
        <li><p>{{this}}</p></li>
      {{/each}}
    </ul>
    <ul>
      {{#each stats}}
        <li>{{season}}::{{goals}}</li>
      {{/each}}
    </ul>
  </li>
{{/each}}